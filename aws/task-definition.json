{
    "family": "definition-test-deploy",
    "containerDefinitions": [
        {
            "name": "laravel-2",
            "image": "906146266574.dkr.ecr.us-east-1.amazonaws.com/test-deploy",
            "cpu": 512,
            "memory": 1024,
            "memoryReservation": 1024,
            "portMappings": [
                {
                    "name": "main",
                    "containerPort": 8080,
                    "hostPort": 80,
                    "protocol": "tcp",
